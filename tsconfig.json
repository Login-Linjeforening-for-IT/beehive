{
  "compilerOptions": {
    "lib": ["ESNext", "dom", "dom.iterable"],
    "target": "ESNext",
    "module": "Preserve",
    "moduleDetection": "force",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noPropertyAccessFromIndexSignature": false,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@context/*": [
        "./src/context/*"
      ],
      "@svg/*": [
        "./src/components/svg/*"
      ],
      "@config": [
        "./constants.ts"
      ],
      "@img/*": [
        "./public/img/*"
      ],
      "@app/*": [
        "./src/app/*"
      ],
      "@text/*": [
        "./public/text/*"
      ],
      "@assets/*": [
        "./public/assets/*"
      ],
      "@themes/*": [
        "./src/themes/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@interfaces": [
        "./interfaces.ts"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "**/*.js",
    "**/*.jsx",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "src/vendor"
  ]
}
